{
  "name": "electron-boilerplate",
  "description": "A boilerplate application for Electron runtime",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/electron-boilerplate.git"
  },
  "author": "iceStone <ice@micua.com> (http://blog.wedn.net/)",
