{
  "name": "electron-boilerplate",
  "description": "A boilerplate application for Electron runtime",
  "version": "0.2.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/electron-boilerplate.git"
  },
  "author": "iceStone <ice@micua.com> (http://blog.wedn.net/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zce/electron-boilerplate/issues"
  },
  "homepage": "https://github.com/zce/electron-boilerplate#readme",
  "scripts": {
    "postinstall": "cd app && npm install && cd ..",
    "// ": "The original usage",
    "// clean:npm": "rimraf node_modules && rimraf **/node_modules",
    "// watch": "cross-env NODE_ENV=development webpack-dev-server --config tasks/webpack.config.renderer.js --hot --inline --colors",
    "// build:main": "webpack --progress --hide-modules --config tasks/webpack.config.main.js",
    "// build:renderer": "webpack --progress --hide-modules --config tasks/webpack.config.renderer.js",
    "// build": "npm run clean && cross-env NODE_ENV=production npm run build:main && cross-env NODE_ENV=production npm run build:renderer",
    "// preboot": "cross-env NODE_ENV=development npm run compile:main",
    "// boot": "cross-env NODE_ENV=development electron build",
    "// prestart": "cross-env NODE_ENV=production npm run compile",
    "// start": "cross-env NODE_ENV=production electron build",
    "// prerelease": "npm run build",
    "// release": "node tasks/release",
    "clean": "gulp clean",
    "clean:npm": "gulp clean:npm",
    "watch": "cross-env NODE_ENV=development gulp watch",
    "build": "cross-env NODE_ENV=production gulp compile:renderer",
    "dist": "cross-env NODE_ENV=production gulp dist",
    "release": "cross-env NODE_ENV=production gulp release",
    "release:all": "cross-env NODE_ENV=production gulp release --platform=all --arch=all",
    "boot": "cross-env NODE_ENV=development gulp boot",
    "start": "cross-env NODE_ENV=production gulp start"
  },
  "dependencies": {
    "keen-ui": "^0.8.8",
    "vue": "^1.0.26",
    "vue-i18n": "^4.1.0",
    "vue-router": "^0.7.13",
    "vuex": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "bluebird": "^3.4.1",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.3.1",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^6.0.0-beta.2",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-gzip": "^1.4.0",
    "gulp-jsonminify": "^1.0.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  }
}
