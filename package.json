{
  "name": "electron-boilerplate",
  "productName": "Electron Boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate application with Vue.js framework for Electron runtime",
  "keywords": [
    "boilerplate",
    "electron",
    "zce"
  ],
  "homepage": "https://github.com/zce/electron-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/zce/electron-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/electron-boilerplate.git"
  },
  "license": "MIT",
  "author": {
    "name": "zce",
    "email": "w@zce.me",
    "url": "https://zce.me"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"**/*.{ts,vue}\" --format node_modules/eslint-friendly-formatter",
    "compile": "electron-webpack",
    "compile:dll": "yarn compile dll",
    "build": "yarn compile:dll && yarn compile && electron-builder",
    "build:dir": "yarn build --dir -c.compression=store -c.mac.identity=null",
    "build:all": "yarn build --macos --linux --windows",
    "dev": "electron-webpack dev",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.6",
    "@commitlint/config-conventional": "8.3.6",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "commitizen": "3.1.2",
    "cz-conventional-changelog": "2.1.0",
    "electron": "^5.0.5",
    "electron-builder": "20.44.4",
    "electron-store": "4.0.0",
    "electron-unhandled": "2.2.0",
    "electron-updater": "4.6.5",
    "electron-util": "0.12.3",
    "electron-webpack": "2.8.2",
    "eslint": "6.8.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-vue": "5.2.3",
    "fork-ts-checker-webpack-plugin": "1.6.0",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "node-sass": "^4.12.0",
    "rimraf": "2.7.1",
    "sass-loader": "7.3.1",
    "standard-version": "^6.0.1",
    "ts-loader": "6.2.2",
    "typescript": "3.9.10",
    "vue": "2.7.10",
    "vue-html-loader": "1.2.4",
    "vue-i18n": "8.27.2",
    "vue-loader": "15.10.0",
    "vue-property-decorator": "8.5.1",
    "vue-router": "3.6.5",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.7.10",
    "vuex": "3.6.2",
    "vuex-class": "0.3.2",
    "webpack": "4.46.0",
    "webpack-build-notifier": "1.2.2"
  }
}
